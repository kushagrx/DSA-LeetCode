class Solution {
public:
    int lengthOfLongestSubstring(string s) {
        int maxlen=0;
        int n=s.length();
        unordered_map<char,int> seen;
        int left=0;
        
        for(int right=0;right<n;right++){
            seen[s[right]]++;
            while(seen[s[right]]>1){
                seen[s[left]]-=1;
                left++;
            }
            maxlen=max(maxlen,right-left+1);
        }

    return maxlen;
    }
};
