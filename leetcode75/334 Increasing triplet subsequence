class Solution {
public:
    bool increasingTriplet(vector<int>& nums) {
        int min=INT_MAX, max=INT_MAX;

        for(auto it:nums){
            if(it<=min){
                min=it;
            }
            else if(it<=max){
                max=it;
            }
            else if(it>max){
                return true;
            }
        }
        return false;
    }
};
