class Solution {
public:
    vector<vector<string>> groupAnagrams(vector<string>& strs) {
        vector<vector<string>>res;
        unordered_map<string,vector<string>>map;

        for(auto it:strs){
            auto temp=it;
            sort(it.begin(),it.end());
            map[it].push_back(temp);
        }
        for(auto it:map){
            res.push_back(it.second);
        }

        return res;
    }
};
