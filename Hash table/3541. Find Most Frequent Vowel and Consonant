class Solution {
public:
    int maxFreqSum(string s) {
        unordered_map<char,int> freq;

        for(char c : s){
            freq[c]++;
        }
        int maxVowel = 0;
        int maxConsonant = 0;

        for(auto it : freq){
            char c = it.first;
            int count = it.second;

            if(c=='a' || c=='e' || c=='i' || c=='o' || c=='u'){
                maxVowel = max(maxVowel, count);
            } else {
                maxConsonant = max(maxConsonant, count);
            }
        }

        return maxVowel + maxConsonant;
    }
};
